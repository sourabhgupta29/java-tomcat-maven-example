pipeline {
    agent any
    stages {
        stage ('Initialize') {
            steps {
                sh '''
                echo "Path = ${Path}"
                echo "M2_HOME = ${M2_HOME}"
                echo  "Initializing the Code File"
                '''
            }
        }

        stage ('Build') {
            steps {
                echo 'Hello World'
            }
        }

    }
}
